<% if(success.length || error.length) {%> 
<div class="container-fluid px-lg-4 px-xl-5">
    <% if(success && success.length) {%> 
        <div class="alert alert-success"> <%= success %> </div>
    <% }; %>  
    <% if(error && error.length) {%> 
        <div class="alert alert-danger"> <%= success %> </div>
    <% }; %>  
</div>
<% } %> 